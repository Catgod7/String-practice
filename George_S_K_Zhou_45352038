WITH T AS (
SELECT 'JDK134.6' AS A FROM DUAL UNION ALL 
SELECT 'KJ12.564' AS A FROM DUAL UNION ALL 
SELECT 'YLEJ64' AS A FROM DUAL 
)
SELECT A, 
SUBSTR(REGEXP_REPLACE(CASE INSTR(A,'.',1) WHEN 0 THEN A ||'.00' ELSE A || '00' END, '[a-zA-Z]', ''),1,INSTR(REGEXP_REPLACE(CASE INSTR(A,'.',1) WHEN 0 THEN A ||'.00' ELSE A || '00' END, '[a-zA-Z]', ''),'.',1)+2) AS E
FROM T 
